{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "build": {
    "env": {
      "LANG": "C.UTF-8",
      "LC_ALL": "C.UTF-8"
    }
  },
  "rewrites": [
    {"source": "/v2/:path*", "destination": "https://api.vkeys.cn/v2/:path*"},
    {"source": "/volc/:path*", "destination": "https://translate.volcengine.com/:path*"},
    {"source": "/doubao/:path*", "destination": "https://www.doubao.com/:path*"},
    {"source": "/(.*)", "destination": "/index.html"}
  ],
  "headers": [
    {"source": "/assets/(.*)", "headers": [{"key": "Cache-Control", "value": "public, max-age=31536000, immutable"}]},
    {"source": "/favicon.ico", "headers": [{"key": "Cache-Control", "value": "public, max-age=86400"}]}
  ]
}
